{% extends 'base.html.twig' %}

{% block title %}QuestionSupport{% endblock %}

{% block body %}
    <h1>QuestionSupport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ question_support.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ question_support.titre }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ question_support.message }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ question_support.date ? question_support.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_question_support_index') }}">back to list</a>

    <a href="{{ path('app_question_support_edit', {'id': question_support.id}) }}">edit</a>

    {{ include('question_support/_delete_form.html.twig') }}
{% endblock %}
